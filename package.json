{
  "name": "pouchdb-thrifty",
  "version": "0.0.1",
  "description": "Optimize synchronization to a remote database by minimizing the number of requests",
  "main": "src/js/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/hermitdemschoenenleben/pouchdb-thrifty.git"
  },
  "keywords": [
    "pouch",
    "pouchdb",
    "plugin",
    "thrifty"
  ],
  "author": "",
  "license": "Apache",
  "bugs": {
    "url": "https://github.com/hermitdemschoenenleben/pouchdb-thrifty/issues"
  },
  "scripts": {
    "test-node": "TEST_DB=testdb,http://localhost:5984/testdb istanbul test ./node_modules/mocha/bin/_mocha test/test.js",
    "test-browser": "./bin/test-browser.js",
    "jshint": "jshint -c .jshintrc *.js test/test.js",
    "test": "npm run jshint && ./bin/run-test.sh",
    "build": "mkdirp dist && mkdirp src/js && tsc && npm run rollup",
    "rollup": "rollup -c rollup-config.js > dist/pouchdb.thrifty.js",
    "dev": "rollup test/test.js > test/test-bundle.js && npm run dev-server",
    "dev-server": "./bin/dev-server.js",
    "coverage": "npm test --coverage && istanbul check-coverage --lines 100 --function 100 --statements 100 --branches 100"
  },
  "dependencies": {
    "events": "^1.1.1",
    "inherits": "^2.0.3",
    "pouchdb-errors": "^6.0.7",
    "pouchdb-generate-replication-id": "^6.0.7",
    "pouchdb-promise": "^6.0.7"
  },
  "devDependencies": {
    "bluebird": "^3.4.0",
    "chai": "^3.5.0",
    "derequire": "^2.0.0",
    "es3ify": "^0.1.3",
    "http-server": "^0.5.5",
    "istanbul": "^0.2.7",
    "jshint": "2.8.0",
    "mkdirp": "^0.5.0",
    "mocha": "^2.5.3",
    "phantomjs": "^1.9.7-5",
    "pouchdb": "^5.4.0",
    "request": "^2.36.0",
    "rollup": "^0.36.3",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-node-builtins": "^2.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "sauce-connect-launcher": "^0.4.2",
    "selenium-standalone": "^5.0.0",
    "typescript": "^2.1.0-dev.20161103",
    "uglify-js": "^2.4.13",
    "watchify": "^3.1.0",
    "wd": "^0.2.21"
  }
}
